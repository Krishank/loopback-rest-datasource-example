{
  "db": {
    "name": "db",
    "connector": "memory"
  },
  "geoDetails": {
    "name": "geoDetails",
    "connector": "rest",
    "options": {
      "headers": {
        "accept": "application/json",
        "content-type": "application/json"
      },
      "strictSSL": false
    },
    "operations": [
      {
        "template": {
          "method": "GET",
          "url": "https://maps.googleapis.com/maps/api/geocode/{format=json}",
          "headers": {
            "accepts": "application/json",
            "content-type": "application/json"
          },
          "query": {
            "address": "{street},{city},{zipcode}",
            "sensor": "{sensor=false}",
            "key": "${API_KEY}"
          },
          "responsePath": "$.results[0]"
        },
        "functions": {
          "geocode": [
            "street",
            "city",
            "zipcode"
          ]
        }
      }
    ]
  }
}